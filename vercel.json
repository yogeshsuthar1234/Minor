{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "client/build/$1"
    }
  ],
  "functions": {
    "server/**/*.py": {
      "runtime": "python3.11"
    }
  },
  "outputDirectory": "client/build"
}